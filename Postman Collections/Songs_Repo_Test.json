		{
			"name": "TimeFilter",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "35d444cc-e9ef-45e3-868c-674c9af26533",
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const songs = pm.response.json().songs;\r",
							"const allFilters = JSON.parse(pm.collectionVariables.get(\"allFilters\"));\r",
							"\r",
							"function returnFilter(filterObj) {\r",
							"    const filter = Object.keys(filterObj)[0];\r",
							"    const filterType = filter.split(\".\");\r",
							"    return filterType[filterType.length-1];\r",
							"};\r",
							"\r",
							"function returnFilterValue(filterObj) {\r",
							"    const filterValue = Object.values(filterObj)[0];\r",
							"    return filterValue;\r",
							"};\r",
							"\r",
							"let anyCharacterMatch = \".*\";\r",
							"\r",
							"function testFilters(song) {\r",
							"    const songStr = JSON.stringify(song);\r",
							"    console.log(songStr);\r",
							"    allFilters.forEach(filter => {\r",
							"        let filterType = returnFilter(filter);\r",
							"        let value = returnFilterValue(filter);\r",
							"        let pattern = new RegExp(`${filterType}${anyCharacterMatch}${value}`);\r",
							"        pm.test(`Song should return song with property ${filterType} and value ${value}`, function () {\r",
							"            pm.expect(songStr).to.match(pattern);\r",
							"        });\r",
							"    });\r",
							"};\r",
							"\r",
							"songs.forEach(song=>{\r",
							"    testFilters(song);\r",
							"});\r",
							"\r",
							"// let currentIteration = pm.info.iteration+1;\r",
							"// console.log(currentIteration);\r",
							"// let totalIteration = Number(pm.info.iterationCount);\r",
							"// console.log(totalIteration);\r",
							"\r",
							"// currentIteration <= totalIteration ? postman.setNextRequest(\"TimeFilter\") : postman.setNextRequest(\"songs\") ;"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "77b3e8d6-c773-4c0c-bb1e-3d0746abba43",
						"exec": [
							"const filters = pm.iterationData.get(\"filters\");\r",
							"pm.collectionVariables.set(\"allFilters\", JSON.stringify(filters));\r",
							"\r",
							"function addParamsToUrl() {\r",
							"    let params = [];\r",
							"    filters.forEach(filter => {\r",
							"        const keys = Object.keys(filter);\r",
							"        const values = Object.values(filter);\r",
							"        keys.forEach((key, index) => {\r",
							"            let param = key + \"=\" + values[index];\r",
							"            params.push(param);\r",
							"        });\r",
							"    });\r",
							"    pm.request.url.addQueryParams(params);\r",
							"};\r",
							"\r",
							"addParamsToUrl();\r",
							"\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
